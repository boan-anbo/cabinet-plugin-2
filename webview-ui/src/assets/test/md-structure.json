    "lineType": "BULLET_POINT",
    "listItemInfo": null,
    "headerLevel": 0,
    "parentHeaderLevel": 1,
    "hasParent": true,
    "content": "So censorship is a technology to shape meaning in the public discourse",
    "line": 44,
    "lineEndSelf": 45,
